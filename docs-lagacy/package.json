{
  "devDependencies": {
    "docsify-cli": "^4.4.4",
    "now": "^21.0.1"
  },
  "name": "my-project",
  "scripts": {
    "start": "docsify serve . -c ssr.config.js",
    "deploy": "now -p"
  },
  "files": [
    "docs"
